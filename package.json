{
  "name": "social-data-listener",
  "version": "1.0.0",
  "description": "Fetches data from several platforms and stores it in the database for further processing",
  "main": "dist/main.js",
  "engines": {
    "node": "16.x.x",
    "npm": "8.1.3"
  },
  "scripts": {
    "test": "jest",
    "prepare": "husky install",
    "build": "npm run update:lib && rimraf dist && tsc",
    "start": "node dist/main.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "ts-node src/main.ts",
    "watch": "nodemon src/main.ts",
    "update:lib": "npm update @infinityxyz/lib@latest",
    "deploy:dev": "npm run build && gcloud app deploy -q --appyaml=./app.yaml --project=nftc-dev",
    "deploy:prod": "npm run build && gcloud app deploy -q --appyaml=./app.yaml --project=nftc-infinity"
  },
  "keywords": [],
  "author": "",
  "license": "",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/multer": "^1.4.7",
    "@types/node-fetch": "^2.6.2",
    "@types/node-schedule": "^1.3.2",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.5",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@discordjs/builders": "^1.1.0",
    "@discordjs/rest": "^1.0.1",
    "@infinityxyz/lib": "^1.232.2",
    "discord-api-types": "^0.37.2",
    "discord.js": "^14.2.0",
    "dotenv": "^16.0.0",
    "firebase-admin": "^11.4.1",
    "husky": "^7.0.4",
    "node-fetch": "^2.6.7",
    "node-schedule": "^2.1.0",
    "phin": "^3.6.1",
    "twitter-api-v2": "^1.11.0"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 130,
    "singleQuote": true,
    "trailingComma": "none",
    "semi": true
  },
  "lint-staged": {
    "src/**/*": "prettier --write --ignore-unknown"
  }
}
