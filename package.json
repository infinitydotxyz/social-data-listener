{
  "name": "social-data-listener",
  "version": "1.0.0",
  "description": "Fetches data from several platforms and stores it in the database for further processing",
  "main": "dist/main.js",
  "scripts": {
    "test": "jest",
    "prepare": "husky install",
    "build": "rimraf dist && tsc",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "node dist/index.js",
    "start:dev": "ts-node src/main.ts",
    "watch": "nodemon src/main.ts",
    "script:twitter": "npx ts-node src/scripts/get-twitter-oauth-tokens.ts",
    "script:login": "npx ts-node -r dotenv/config src/scripts/create-bot-account.ts",
    "update:lib": "npm update @infinityxyz/lib"
  },
  "keywords": [],
  "author": "",
  "license": "",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node-schedule": "^1.3.2",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.5",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@discordjs/rest": "^0.3.0",
    "@infinityxyz/lib": "^1.32.4",
    "chalk": "^4.1.2",
    "discord-api-types": "^0.29.0",
    "discord.js": "^13.6.0",
    "emittery": "^0.10.2",
    "express": "^4.17.3",
    "firebase-admin": "^10.0.2",
    "node-schedule": "^2.1.0",
    "p-queue": "^6.6.2",
    "phin": "^3.6.1",
    "twitter-api-v2": "^1.11.0",
    "uuid": "^8.3.2"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 130,
    "singleQuote": true,
    "trailingComma": "none",
    "semi": true
  },
  "lint-staged": {
    "src/**/*": "prettier --write --ignore-unknown"
  }
}
